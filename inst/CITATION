citHeader("To cite Kernel Factory use:")

citEntry(
 entry="Article",
 title="Kernel Factory: An Ensemble of Kernel Machines",
 journal="Expert Systems With Applications",
 volume= "Forthcoming",
 number="",
 pages="",
 year="2013",
 author=personList(person("Michel Ballings"),
			 person("Dirk Van den Poel")),                                  
 textVersion="Ballings, M., & Van den Poel, D. (2012). Kernel Factory: An ensemble of Kernel Machines. Expert Systems With Applications. Forthcoming." 
 )
 
